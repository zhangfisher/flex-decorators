{
    "name": "flex-decorators",
    "version": "1.0.3",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "homepage": "https://zhangfisher.github.com/flex-decorators",
    "repository": "https://zhangfisher.github.com/flex-decorators",
    "scripts": {
        "build": "tsup",
        "test": "pnpm vitest",
        "release": "npm version patch  && npm publish"
    },
    "files": [
        "dist/",
        "./"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./timeout": {
            "import": "./dist/decorators/timeout.mjs",
            "require": "./dist/decorators/timeout.js",
            "default": "./dist/decorators/timeout.js",
            "types": "./dist/decorators/timeout.d.ts"
        },
        "./debounce": {
            "import": "./dist/decorators/debounce.mjs",
            "require": "./dist/decorators/debounce.js"
        },
        "./deprecate": {
            "import": "./dist/decorators/deprecate.mjs",
            "require": "./dist/decorators/deprecate.js"
        },
        "./memorize": {
            "import": "./dist/decorators/memorize.mjs",
            "require": "./dist/decorators/memorize.js"
        },
        "./noReentry": {
            "import": "./dist/decorators/noReentry.mjs",
            "require": "./dist/decorators/noReentry.js"
        },
        "./queue": {
            "import": "./dist/decorators/queue.mjs",
            "require": "./dist/decorators/queue.js"
        },
        "./reliable": {
            "import": "./dist/decorators/reliable.mjs",
            "require": "./dist/decorators/reliable.js"
        },
        "./retry": {
            "import": "./dist/decorators/retry.mjs",
            "require": "./dist/decorators/retry.js"
        },
        "./throttle": {
            "import": "./dist/decorators/throttle.mjs",
            "require": "./dist/decorators/throttle.js"
        },
        "./verifyArgs": {
            "import": "./dist/decorators/verifyArgs.mjs",
            "require": "./dist/decorators/verifyArgs.js"
        },
        "./asyncSignal": {
            "import": "./dist/asyncSignal.mjs",
            "require": "./dist/asyncSignal.js"
        },
        "./liteEventEmitter": {
            "import": "./dist/liteEventEmitter.mjs",
            "require": "./dist/liteEventEmitter.js"
        },
        "./wrappers/*": {
            "import": "./dist/wrappers/*.mjs",
            "require": "./dist/wrappers/*.js"
        },
        "./decorators": {
            "import": "./dist/decorators/index.mjs",
            "require": "./dist/decorators/index.js"
        },
        "./decorators/*": {
            "import": "./dist/decorators/*.mjs",
            "require": "./dist/decorators/*.js"
        }
    },
    "typesVersions": {
        "*": {
            "timeout": [
                "./dist/decorators/timeout.d.ts"
            ],
            "debounce": [
                "./dist/decorators/debounce.d.ts"
            ],
            "deprecate": [
                "./dist/decorators/deprecate.d.ts"
            ],
            "memorize": [
                "./dist/decorators/memorize.d.ts"
            ],
            "throttle": [
                "./dist/decorators/throttle.d.ts"
            ],
            "noReentry": [
                "./dist/decorators/noReentry.d.ts"
            ],
            "queue": [
                "./dist/decorators/queue.d.ts"
            ],
            "reliable": [
                "./dist/decorators/reliable.d.ts"
            ],
            "retry": [
                "./dist/decorators/retry.d.ts"
            ],
            "verifyArgs": [
                "./dist/decorators/verifyArgs.d.ts"
            ],
            "asyncSignal": [
                "./dist/asyncSignal.d.ts"
            ],
            "liteEventEmitter": [
                "./dist/liteEventEmitter.d.ts"
            ],
            "decorators": [
                "./dist/decorators/index.d.ts"
            ],
            "wrappers": [
                "./dist/wrappers/*.d.ts"
            ]
        }
    },
    "dependencies": {
        "reflect-metadata": "^0.1.13",
        "throttle-debounce": "^5.0.0"
    },
    "devDependencies": {
        "@swc/core": "^1.3.9",
        "@types/throttle-debounce": "^5.0.0",
        "@vitest/coverage-c8": "^0.23.4",
        "@vitest/ui": "^0.23.4",
        "ansi-colors": "^4.1.3",
        "docsify": "^4.12.2",
        "ts-node": "^10.9.1",
        "tsup": "^6.2.3",
        "type-fest": "^2.19.0",
        "typescript": "^4.8.3",
        "vitest": "^0.23.4"
    }
}
