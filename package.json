{
    "name": "flex-decorators",
    "version": "0.0.1",
    "dependencies": {
        "reflect-metadata": "^0.1.13",
        "throttle-debounce": "^5.0.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./decorators/*": {
            "import": "./dist/decorators/*.mjs",
            "require": "./dist/decorators/*.js"
        },
        "./wrappers/*": {
            "import": "./dist/wrappers/*.mjs",
            "require": "./dist/wrappers/*.js"
        },
        "./asyncSignal": {
            "import": "./dist/asyncSignal.mjs",
            "require": "./dist/asyncSignal.js"
        },
        "./eventEmitter": {
            "import": "./dist/eventEmitter.mjs",
            "require": "./dist/eventEmitter.js"
        }
    },
    "devDependencies": {
        "@types/throttle-debounce": "^5.0.0",
        "@vitest/coverage-c8": "^0.23.4",
        "@vitest/ui": "^0.23.4",
        "docsify": "^4.12.2",
        "ts-node": "^10.9.1",
        "tsup": "^6.2.3",
        "type-fest": "^2.19.0",
        "typescript": "^4.8.3",
        "vitest": "^0.23.4"
    }
}
